{
	"manifest_version": 2,

	"name": "Wtyczka streamera!",
	"description": "Powiadamiacz streamerski - Czy stream jest online?",
	"version": "0.1",

	"browser_action":{
		"default_title": "",
		"default_popup": "popup.html",
    	"default_icon": "icons/dead_glitch.png"
	},

	"permissions":[
		"https://api.twitch.tv/kraken",
		"storage",
		"notifications"
	],

	"background": {
	    "scripts": ["background.js"]
	}
}